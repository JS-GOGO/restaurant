<!--components/home.js/x-swiper.wxml-->
<swiper class="swiper" indicator-dots indicator-active-color="#ff8198" autoplay interval="1500" circular>
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image src="{{item.image}}" mode="scaleToFill"></image>
    </swiper-item>
  </block>
</swiper>
